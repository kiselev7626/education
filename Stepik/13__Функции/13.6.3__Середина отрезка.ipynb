    "# объявление функции\n",
    "def get_middle_point(x1, y1, x2, y2):\n",
    "    x=(x1+x2)/2\n",
    "    y=(y1+y2)/2\n",
    "    return x, y\n",
    "\n",
    "# считываем данные\n",
    "x_1, y_1 = int(input()), int(input())\n",
    "x_2, y_2 = int(input()), int(input())\n",
    "\n",
    "# вызываем функцию\n",
    "x, y = get_middle_point(x_1, y_1, x_2, y_2)\n",
    "print(x, y)"
   ]
  }
