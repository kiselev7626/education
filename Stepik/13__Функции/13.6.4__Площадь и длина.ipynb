    "# объявление функции\n",
    "def get_circle(radius):\n",
    "    import math\n",
    "    length = 2 * math.pi * radius\n",
    "    square = math.pi * radius **2\n",
    "    return length, square\n",
    "\n",
    "# считываем данные\n",
    "r = float(input())\n",
    "\n",
    "# вызываем функцию\n",
    "length, square = get_circle(r)\n",
    "print(length, square)"
   ]
